    "def countKeyChanges(self, s: str) -> int:\n",
    "    if len(s) == 1:\n",
    "        return 0\n",
    "    s1 = s.lower()\n",
    "    c = 0\n",
    "    for i in range(len(s1)-1):\n",
    "        if s1[i+1] != s1[i]:\n",
    "            c += 1\n",
    "    return c "
   ]
  }
